<%= form_for :session, url: sign_in_path do |f| %>
  <div class="nav">
    <h1>Conecte-se!</h1>
    <h2>Crie quizzes quando quiser</h2>
    <h3>Busque seus c√≥digos de jogos</h3>
  </div>
  <div class="container login-container">
    <h4>Insira seus dados</h4>
    <%= f.email_field :email, autofocus: true, placeholder: 'E-mail', class:'form-control' %>
    <%= f.label :email, "Insira o seu email" %>
    <%= f.password_field :password, autocomplete: "off", placeholder: 'Senha', class:'form-control' %>
    <%= f.label :password, "Insira sua senha secreta" %>
    <br>
    <%= f.submit "Entrar", class: 'btn btn-primary' %>
  <div>

  <br>
  <%= link_to "Cadastre-se", new_user_path %>
<% end %>
